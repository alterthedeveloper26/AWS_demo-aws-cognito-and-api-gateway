    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "winston-syslog": "^2.5.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
