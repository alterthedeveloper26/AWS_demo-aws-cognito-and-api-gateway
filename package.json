  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist && rimraf tsconfig.build.tsbuildinfo",
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn build && node --max_old_space_size=2560 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn build && yarn typeorm migration:generate -p -o -n",
    "migration:create": "yarn typeorm migration:create -o -n",
    "db:init": "yarn migration:generate init-db",
    "db:migrate": "yarn typeorm migration:run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.171.0",
